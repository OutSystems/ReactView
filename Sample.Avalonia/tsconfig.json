{
  "compilerOptions": {
    "incremental": true,
    "baseUrl": ".",
    "module": "amd",
    "target": "es2017",
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": false,
    "allowJs": true,
    "rootDir": ".",
    "forceConsistentCasingInFileNames": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "exclude": [
    "dist",
    "Generated",
    "node_modules"
  ]
}